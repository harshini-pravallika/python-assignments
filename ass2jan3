n=int(input('enter matrix size: '))
matrix=[]
for i in range(n):
    l=[]
    for j in range(n):
        x=int(input('enterv a number: '))
        l.append(x)
    matrix.append(l)
for i in matrix:    
    print(i)    
for i in range(n):
    for j in range(n):
        if i==j:
            print(matrix[i][j])
