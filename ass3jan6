email=input('enter email id: ')
phno=input('enter phno: ')
c=0
f=int(phno[0:2])
for i in phno:
    if i.isdigit():
        c=c+1
if email.endswith("@gmail.com") or email.endswith("@yahoo.com") or email.endswith("@adc.aditya.ac.in") and c==11 and f>5 and phno[0]=='0':
    print(' Registration successully')
else:
    print('check your details')
