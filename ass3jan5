n=int(input('enter matrix size: '))
matrix=[]
for i in range(n):
    l=[]
    for j in range(n):
        x=int(input('enter a number: '))
        l.append(x)
    matrix.append(l)
count=0    
for i in range(n):
    sum=0
    for j in range(n):
        sum=sum+matrix[i][j]
    if sum==2:
        count=count+1
if count==n:
    print('coder target matrix')
else:
    print('not coder target matrix')
